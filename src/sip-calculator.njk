<!-- Chart.js CDN (Specific to this page) -->
{% block head %}
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
{% endblock %}

<script>
    let myChart = null;

    function formatCurrency(amount) {
        if (amount >= 10000000) {
            return '₹' + (amount / 10000000).toFixed(2) + ' Cr';
        } else if (amount >= 100000) {
            return '₹' + (amount / 100000).toFixed(2) + ' Lakh';
        } else {
            return '₹' + amount.toLocaleString('en-IN');
        }
    }

    function calculateSIP() {
        // Get input values
        const p = parseFloat(document.getElementById('monthlyInvestment').value);
        const r = parseFloat(document.getElementById('returnRate').value);
        const t = parseFloat(document.getElementById('timePeriod').value);

        // Calculate
        const i = r / 12 / 100;
        const n = t * 12;

        let fv;
        if (i === 0) {
            fv = p * n;
        } else {
            fv = p * ((Math.pow(1 + i, n) - 1) / i) * (1 + i);
        }
        
        const invested = p * n;
        const returns = fv - invested;

        // GA4 Event Tracking
        trackCalculatorUsage('SIP Calculator', p);

        // Update UI
        document.getElementById('totalValue').innerText = formatCurrency(fv);
        document.getElementById('investedAmount').innerText = formatCurrency(invested);
        document.getElementById('estReturns').innerText = formatCurrency(returns);

        updateChart(invested, returns);
    }

    function updateChart(invested, returns) {
        const ctx = document.getElementById('sipChart').getContext('2d');

        if (myChart) {
            myChart.destroy();
        }

        myChart = new Chart(ctx, {
            type: 'doughnut',
            data: {
                labels: ['Invested Amount', 'Est. Returns'],
                datasets: [{
                    data: [invested, returns],
                    backgroundColor: [
                        '#E0E7FF', // Light Indigo
                        '#4F46E5'  // Indigo
                    ],
                    borderWidth: 0,
                    hoverOffset: 4
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: {
                        display: false
                    },
                    tooltip: {
                        callbacks: {
                            label: function (context) {
                                let label = context.label || '';
                                if (label) {
                                    label += ': ';
                                }
                                label += formatCurrency(context.raw);
                                return label;
                            }
                        }
                    }
                },
                cutout: '70%'
            }
        });
    }

    // Initialize on load
    document.addEventListener('DOMContentLoaded', () => {
        initAutoSave('sip_calc');
        calculateSIP();
    });
</script>
