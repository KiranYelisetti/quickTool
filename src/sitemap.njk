---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for page in collections.all %}
  {% if not page.data.eleventyExcludeFromCollections %}
  <url>
    <loc>https://www.quicktoolsindia.com{{ page.url | url }}</loc>
    <lastmod>{{ page.date.toISOString().split('T')[0] }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>{% if page.url == '/' %}1.0{% elif 'calculator' in page.url %}0.9{% else %}0.8{% endif %}</priority>
  </url>
  {% endif %}
{% endfor %}
</urlset>
